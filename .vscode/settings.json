{
    "python.autoComplete.extraPaths": ["code"],
    "python.analysis.extraPaths": ["code"],
    "python.analysis.diagnosticSeverityOverrides":{
        "reportMissingImports": "none"
    },
    "files.encoding": "utf8",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/build": true,
        "**/dist": true,
        "**/*.spec": true,
        "**/__pycache__": true,
        "**/ExeFile/build": true,
        "**/ExeFile/dist": true,
        "**/*.exe": true,
        "**/*.ico": true,
        "**/.venv": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/build": true,
        "**/dist": true,
        "**/*.spec": true,
        "**/__pycache__": true,
        "**/ExeFile/build": true,
        "**/ExeFile/dist": true,
        "**/*.exe": true,
        "**/*.ico": true,
        "**/.venv": true
    },
    "python.analysis.typeCheckingMode": "basic",
    "qtForPython.designer.path": ".venv/Lib/site-packages/qt5_applications/Qt/bin/designer.exe",
    "qtForPython.uic.liveExecution": true,
    "qtForPython.uic.path": "pyuic5",
    "qtForPython.uic.args":["-o ./GUI/QtGuiGenerate.py"]
}
